<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.styd.application">
    <!--获取登录接口case-->
    <select id="getagreement" parameterType="Integer" resultType="String">
        select content from user_contract where uid=#{uid} and type=1
    </select>

    <!--获取门店信息-->
    <select id="nearbyshop"  resultType="map">
        select id as shop_id,shop_name as shop_name from shop where uid=#{uid}
    </select>
    
