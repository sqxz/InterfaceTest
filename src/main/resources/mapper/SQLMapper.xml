<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.styd.model">
    <!--查询获取公告接口测试数据-->
    <select id="get_agreement" parameterType="Integer" resultType="com.styd.model.GetAgreementCase">
        select * from get_agreement_case
        where id=#{id}
    </select>

    <!--查询查询门店接口测试数据-->
    <select id="nearby_shop" parameterType="Integer" resultType="com.styd.model.NearByShop">
        select * from nearby_shop
        where id=#{id}
    </select>

    <!--批量插入数据-->
    <insert id="insertData" >
        insert into users (id,username,password)
        values (#{id},#{username},#{password})
    </insert>


</mapper>